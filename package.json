{
  "name": "my-template-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "yarn dev:next & yarn dev:typed-scss-modules",
    "dev:next": "next dev",
    "dev:typed-scss-modules": "typed-scss-modules src --watch --implementation sass --exportType default",
    "format": "prettier --check ./src",
    "format:fix": "yarn prettier --write ./src",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "sort-package-json": "sort-package-json",
    "start": "next start",
    "test": "jest --coverage",
    "type-test": "tsc -p tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "next": "12.1.5",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "sanitize.css": "^13.0.0",
    "sass": "^1.53.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@tsconfig/strictest": "^1.0.1",
    "@types/node": "17.0.25",
    "@types/react": "18.0.6",
    "@types/react-dom": "18.0.2",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-storybook": "^0.5.13",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "lint-staged": "^12.4.0",
    "msw": "^0.42.3",
    "prettier": "^2.6.2",
    "sass-loader": "^13.0.1",
    "sort-package-json": "^1.57.0",
    "style-loader": "^3.3.1",
    "typed-scss-modules": "^6.5.0",
    "typescript": "4.6.3"
  },
  "engines": {
    "npm": "Use 'yarn' as the package manager for this project. Not 'npm'."
  },
  "msw": {
    "workerDirectory": "public"
  }
}